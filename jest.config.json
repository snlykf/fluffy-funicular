{
    "testEnvironment": "node",
    "testPathIgnorePatterns": ["dist"],
    "coveragePathIgnorePatterns": [
        "/node_modules/"
    ],
    "setupFilesAfterEnv": [
        "./src/test/setupTests.js"
    ],
    "collectCoverageFrom": ["src/**/*.js", "!**/node_modules/**"],
    "coverageReporters": ["html", "text", "text-summary", "cobertura"]
}
